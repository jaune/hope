{
	"\\.png$": {
		"transform": "webp-lossless"
	},
	"\\.tiles.json$": {
		"transform": ["tile-indexes", "tile-indexes-binary"]
	},
	"ConstructionRecipeTable.json$": {
		"transform": {
			"type": "flatc",
			"schema": "../src/asset/ConstructionRecipeTable.fbs"
		}
	},
	"CraftRecipeTable.json$": {
		"transform": {
			"type": "flatc",
			"schema": "../src/asset/CraftRecipeTable.fbs"
		}
	},
	"MachineTable.json$": {
		"transform": {
			"type": "flatc",
			"schema": "../src/asset/MachineTable.fbs"
		}
	},
	"ItemTable.json$": {
		"transform": {
			"type": "flatc",
			"schema": "../src/asset/ItemTable.fbs"
		}
	}
}